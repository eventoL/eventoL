{
  "name": "links",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "test": "./node_modules/.bin/jest --forceExit --ci --coverage --detectOpenHandles",
    "watch-test": "./node_modules/.bin/jest --watchAll --detectOpenHandles",
    "eslint": "./node_modules/.bin/eslint --ext .jsx --ext .js src",
    "eslint-fix": "./node_modules/.bin/eslint --ext .jsx --ext .js --fix src",
    "eslint-report": "./node_modules/.bin/eslint -f node_modules/eslint-html-reporter/reporter.js -o report.html --ext .jsx --ext .js src || true",
    "sass-lint": "./node_modules/.bin/sass-lint -v -q",
    "sass-lint-report": "./node_modules/.bin/sass-lint -v -q -f JSON | node ./node_modules/scss-lint-html-reporter/bin/reporter.js"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.9.0",
    "eslint-html-reporter": "0.5.2",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.27.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "jest-fetch-mock": "1.7.4",
    "react-test-renderer": "16.6.1",
    "webpack": "1.15.0",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "1.16.5"
  },
  "dependencies": {
    "css-loader": "0.28.11",
    "es6-promise": "4.2.5",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "file-loader": "2.0.0",
    "image-webpack-loader": "3.6.0",
    "imagemin-optipng": "5.2.1",
    "imagemin-svgo": "6.0.0",
    "isomorphic-fetch": "2.2.1",
    "jest-cli": "23.6.0",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "mock-socket": "8.0.5",
    "node-sass": "4.10.0",
    "prop-types": "15.6.2",
    "radium": "0.24.1",
    "raf": "3.4.1",
    "react": "16.6.1",
    "react-cookie": "3.0.4",
    "react-csv": "1.0.18",
    "react-dom": "16.6.1",
    "react-input-toggle": "0.4.0",
    "react-redux": "5.1.1",
    "react-sizes": "1.0.4",
    "react-slick": "0.16.0",
    "react-table": "6.8.6",
    "react-websocket": "1.2.0",
    "redux": "4.0.1",
    "redux-devtools": "3.4.1",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "resolve-url": "0.2.1",
    "resolve-url-loader": "2.3.1",
    "sass-lint": "1.12.1",
    "sass-loader": "7.1.0",
    "scss-lint-html-reporter": "0.3.2",
    "scss-loader": "0.0.1",
    "slick-carousel": "1.8.1",
    "style-loader": "0.23.1",
    "yarn": "1.12.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTests.jsx",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost:3000",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
